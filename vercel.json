{
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          }
        ]
      },
      {
        "source": "/manifest.json",
        "headers" : [
          {
            "key" : "Content-Type",
            "value" : "application/json; charset=utf-8"
          },
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          }
        ]
      },
      {
        "source": "(.*(css|webp|jpg|woff2|png|svg))",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "public, max-age=604800, immutable"
          },
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          }
        ]
      }
    ]
  }